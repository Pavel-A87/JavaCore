Task3 p12
mport java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double x = input.nextDouble();
		double y = -4 % 3 * (x + (2 + (8 / -2 + 15) + (-x + 4 % 3) * 2) -12 / 4);
		double z = y % 3 + -x * 2;
		
		
		System.out.println( z );
		
	}
}
