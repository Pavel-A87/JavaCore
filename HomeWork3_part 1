Task3 p11
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double one = input.nextDouble();
		double two = input.nextDouble();
		double three = input.nextDouble();
		double fore = input.nextDouble();
		double five = input.nextDouble();
		double sum = one + two + three + fore + five;
		double mult = one * two * three * fore * five;
		boolean sravnim = mult >= sum;
		
		System.out.println( sravnim );
		
	}
}

