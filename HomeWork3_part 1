Task3 p14

import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double x = input.nextDouble();
		double y =  -x + 4 % 3 * 2 - 12 / 4 + (x + (2 + (8 / -2 + 15)));
		boolean z = ! ( y > 100 ) || x > 100 && y > 100;
		
		
		System.out.println( z );
		
	}
}
