Task 3 p8
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double one = input.nextDouble();
		double two = input.nextDouble();
		double summsqrt = (one * one) + (two * two);
		
		System.out.println( summsqrt );
		
	}
}
