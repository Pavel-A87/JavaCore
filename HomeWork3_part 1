Task2 p1-5
package letion3day1;
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double one = input.nextDouble();
		double two = one * 5;
		double three = one + two;
		one = one + three;
		System.out.println( one );
		
	}
}
