Task3 p15
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double x = input.nextDouble();
		boolean y =  x + 4 % 3 - x + (11 -2 / 2 * 0 - (x * -1)) >= x;
		boolean z = ! ( x <= 3 && y == false ) || x >= 3;
		
		
		System.out.println( z );
		
	}
}





