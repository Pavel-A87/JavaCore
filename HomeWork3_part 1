package letion3day1;
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double delimoe = input.nextDouble();
		double delitel = input.nextDouble();
		double ostatok = delimoe%delitel;
		System.out.println( ostatok );
		
	}
}
