Task 3 p7
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double number = input.nextDouble();
		boolean sravn = 20<number && number<100;
		
		System.out.println( sravn );
		
	}
}
