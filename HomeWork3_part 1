Task3 p13
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double x = input.nextDouble();
		double y = (x + (2 +(8 / -2 + 15) +(-x + 4 % 3) * 2) -12 / 4);
		double z = y % 3 + -x * 2;
		
		
		System.out.println( z );
		
	}
}

