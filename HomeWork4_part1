Task 2 p2
{
		Scanner input = new Scanner(System.in);
		
		System.out.print("Zadanie: vivesti na ekran posledovatelnoost 4isel ot 100 do 0 . ");
		System.out.println("Vvedite START i nazhmite ENTER");
		String start = input.next(); 
		int count = 101;
		
		if ( start == start ) {
			while ( count > 0) {
				count = count - 1;
				System.out.println( count );
			
		}
		}	
	}

