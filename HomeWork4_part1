Task4 p18
Scanner input = new Scanner(System.in);
		
System.out.println("Vvvedite razmer posledovatelnosti massiva, N: ");
int n = input.nextInt();

int [] array = new int [n];			// massiv chisel
int index = 0;


while ( index < n ) { // zapolnim massiv
	System.out.println("Vvedite chislo: ");
	int x = input.nextInt();
	array [index] = x;
	index ++;
		}
	System.out.println("1. Vtoroi element massiva: " + array[1]);
		index = 0;			
	int a = 0;
	while ( index < array.length) {
		a = a + array[index];
	index ++;
	
	}
	int b = a%n;
	System.out.println("2. Element kotori nahoditsya v pozitsii, ravnoi ostatku: " );
	System.out.println("   ot deleniya  summi znachenii vseh elementov posledovatelnosti na N: " + array[b]);
	
	int c = 0;
	index = 0;
	
	while(index < array.length) {
		if (array[index]  >= c) {
			c = array[index];
		} 
		index ++;
		
	}
	
	System.out.println("3. Samii bolshoi element posledovatelnosti: " + c );
	
	
	int g = c;
	index = 0;
	
	while(index < array.length) {
		if (array[index]  <= g && array[index] % 6 == 0 ) {
			g = array[index];
		} 
		index ++;
		
	}
	
	System.out.println("3. Samii malenkii element posledovatelnosti kratnii 6: " + g );
	
	
	
	
	
	
//---------------------
	
	
	}
}



