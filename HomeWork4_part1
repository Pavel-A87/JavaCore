Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: polzovatel vvodin n 4isel. Vivesti posledovatelnost n v obratnom poriadke.");
		
		 /*  a**/
		
		
		System.out.println( "Vvedite kolichestvo 4isel v massive, N: " ); 
		int n = input.nextInt();
		int index = 0; 
		double massiv [] = new double[n]; 
		
		while ( index < n ) { 
			System.out.println( "Vvedite chislo: " ); 
			massiv [ index ] = input.nextDouble(); 
			index = index + 1; 
						}
						index = massiv.length - 1; 
							while( index >= 0 ) { 
									System.out.println( massiv [ index ] + " " ); 
									index = index - 1; 
											}

//---------------------
	
	}
}
