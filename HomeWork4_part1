Task 2 p3
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: vivesti na ekran vse chetnie 4isla v diapazone ot A do B. Znacheniya A, B vvodiatsya s klaviaturi.");
		System.out.println("Vvedite znachenie A i nazhmite ENTER");
		int a = input.nextInt(); 
		System.out.println("Vvedite znachenie B i nazhmite ENTER");
		int b = input.nextInt(); 
		int c = a;
	
		if (a > b) {
			System.out.println( "A dolzhno bit bolche B!!!!" );
				} else {
					while ( c <= b ) {
						if ( c % 2 == 0) {
						System.out.println( c );
						c = c + 1;
						}else {
						c = c + 1;
						}
			
		}
		}	
	}
	}
