Task 4 p15
Scanner input = new Scanner(System.in);
		
System.out.println("Vvedite kolichestvo elementov massiva, N: ");
int n = input.nextInt(); 
double array [] = new double[n];
int index = 0; 

while ( index < n ) { 
			System.out.println("Vvedite chislo: "); 
			array[index] = input.nextDouble(); 
			index = index + 1; 
	  				}
					index = array.length - 1; 
					double max = array[0]; 
					double min = array[0];
					while( index >= 0) {
										if(array[index] > max) { 
												max = array[index]; 
												}
										if(array[index] < min) { 
												 min = array[index]; 
												}
										index = index - 1; 
										}
System.out.println("Minimalnoe 4islo iz posledovalelnosti: " + min); 
System.out.println("Maximalnoe 4islo iz posledovalelnosti: " + max);
//---------------------
	
	}
}
