Task 2 p8
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: vivesti na ekran vse 4isla ot 0 do n, ne prevoshodiaschih n ");
		System.out.println("i ne delyaschihsya ni na odno iz 4isel 2, 3, 5. Znachenie n vvoditsya s ");
		System.out.println("klaviatury)");
		System.out.println("Vvedite 4islo N: ");
		int ncount = input.nextInt();
		int sum = 0;
		int n = 0;
	
		
		while ( n < ncount ) {
					if ((n % 2 != 0) && (n % 3 != 0) && (n % 5 != 0)) {
						System.out.println ( n );
						n = n + 1;
						} 
					else {
						n = n + 1;
						}
							}
}
}

