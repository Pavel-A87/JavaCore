Task 4 p16
Scanner input = new Scanner(System.in);
		
double array [] = new double[100];
int maxArray = 100;
int index = 0; 
int n = 0;
int maxN = 7;

while ( index < maxArray ) { 
					if (n < maxN ) {
						n = n + 1;
						array[index] = n; 
						index = index + 1; 
								} else {
									n =0;
								}
								}
								
	  				
					index = 0; 
					while( index < array.length ) {
										System.out.println(array[index]);
										index = index + 1;
										}

//---------------------
	
	}
}
