Task3 p13
public static void main(String[] args) {
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: napisat programmu dlya atelie.");
		
		 /*  a**/
		
		System.out.println("Vvedite dlinnu imeuscheisya tkani, m.: ");
		double dlinna = input.nextDouble();
		double x1;
		double x2;
		double d;
		String s2 = "END";
		String sum = "";
//---------------------
		while ( dlinna > 0){
			System.out.println("Vvedite dlinnu otrezka tkani: ");
			double otrezok = input.nextDouble();
				if( 1 > dlinna ) {
					System.out.print( "Ostatok tkani: " + dlinna + " Otrezat bolshe nelzia" );
				} else {
					if( otrezok < 1) {
					System.out.println( "Tak malo otrezat nelza!!! Minimalnaya dlinna otrezka 1 metr");
				} else {
					if ( dlinna == 0 || otrezok > dlinna) {
						System.out.println( "Tkan zakonchilas!!!" );
				} else {
					dlinna = dlinna - otrezok;
					
				}
			}
		  }
		}
		System.out.println( "Tkan zakoncholas!!!! Ostatok: " + dlinna );


//---------------------
	
	}
}
