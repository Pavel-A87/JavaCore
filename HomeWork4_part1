Task 3 p11
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: napisat programmu reshauschuyu kvadratnoe urovnenie.");
		System.out.println("Koeffitsienti vvodiatsya polzovatelem s klaviaturi");
		 /*  a**/
		
		System.out.println("Vvedite koeffitsienti kvadratnogo urovneniya: A, B, C ");
		System.out.println("Vvedite koeffitsient A: ");
		double a = input.nextDouble();
		System.out.println("Vvedite koeffitsient B: ");
		double b = input.nextDouble();
		System.out.println("Vvedite koeffitsient C: ");
		double c = input.nextDouble();
		double x1;
		double x2;
		double d;
		String s2 = "END";
		String sum = "";
//---------------------
		d = b * b - 4 * a * c;
		if ( a == 0 ){
			System.out.println( "Vi vveli A ravnoe 0. Raschet ne vozmozhen!!!!");
					} else {
						if( d < 0 ) {
				System.out.println( "Kornei net!!!" );
									} else {
								 			if ( d == 0) {
								 				x1 =  - b / (2 * a);
								 				System.out.println( "U urovneniya 1 koren: " + x1 );
								 						} else {
								 								x1 =  (- b + Math.sqrt( d ))/ (2 * a);
								 								x2 =  (- b - Math.sqrt( d ))/ (2 * a);
								 								System.out.println( "U urovneniya 2 korenia: x1= " + x1 + " x2= " + x2 );
								 								}
								 
											}
							}
						}


//---------------------
	
	}

				
