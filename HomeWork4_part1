Task 2 p9
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: vivesti na ekran vse summu kvadratov vseh ne4etnih 4isel iz");
		System.out.println("posledovatelnosti ot A do B. Programma dolzhna proveryat 4to user ");
		System.out.println("vvel A<B");
		
		System.out.println("Uslovie: A<B ");
		System.out.println("Vvedite 4islo A: ");
		int a = input.nextInt();
		System.out.println("Vvedite 4islo B: ");
		int b = input.nextInt();
		int sum = 0;
		int n = a;
	
		if (a > b) {
				System.out.println("Vi vveli nevernie chisla. Po usloviu A dolzno bit menche B");
					} else {
							while ( b >= n ) {
											if (n % 2 != 0) {
															sum = sum + n * n;
															n = n + 1;
															} else {
																	n = n + 1;
																	}
											  }
										System.out.println ( sum );			
							}
	}
}
			

