Task 2 p10
public static void main(String[] args) {
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: npisat programmu kot-ya budet s4itivat N slov i simvolnih virazhenii");
		System.out.println("vvodimih polzovatelem i skleivat ih v odin obschii tekst. Signalom zaversheniya vvoda ");
		System.out.println("i vivoda vsego teksta polzovatelu, dolzhno sluzhit vvedennoe slovo END");
		System.out.println("Zna4enie N vvoditya s klaviaturi. Dlya proverki ispolzovat .equals() iz tipa String");
		
		System.out.println("Vvedite slovo ili simvolnoe virazhenie: ");
		String s1 = "";
		String s2 = "END";
		String sum = "";
		

		 while ( s1.equals(s2) == false ) {
			  s1 = input.next();
			 							if ( s1.equals(s2) == false ) {
			 								sum = sum + s1;
				
			 								} else {
			 									System.out.println( s2 );
			 										}
		 								} 
		 					System.out.println( sum );
	
	}
}
