Task 2 p5
	public static void main(String[] args) {
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: vivesti na ekran kol-vo otricatelnih, kol-vo polozhitelnih i kol-no nulei sradi N vvedennih polzovatelem 4isel. Zna4enie N vvoditsya s klaviaturi");
		System.out.println("Vvedite znachenie N i nazhmite ENTER");
		int ncount = input.nextInt(); // kol-vo chisel
		int pol = 0;// polozhitelnie 4isla
		int otr = 0;// otritsatselnie 4isla
		int nul = 0;// 4isla ravnie nulu
		
		System.out.println("Vvedite luboe chislo i nazhmite ENTER");
							while (ncount > 0) {
								int num = input.nextInt(); //vvodim sravnivoemoe 4islo
								if (num < 0) {
									otr = otr + 1;
											} else {
												if (num == 0) {
													nul = nul +1;
															} else {
																if (num > 0) {
																	pol = pol + 1;
																			}
															}
											}
																ncount = ncount - 1;
											}
							System.out.println( "Polozhitelnih " + pol );
							System.out.println( "Otritsatselnih " + otr );
							System.out.println( "Nulei " + nul );
							}
}
