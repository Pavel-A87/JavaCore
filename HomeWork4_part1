Task 2 p1
{
Scanner input = new Scanner(System.in);
		
		System.out.print("Zadanie: vivesti na ekran posledovatelnoost 4isel ot 0 do 100. ");
		System.out.println("Vvedite START i nazhmite ENTER");
		String start = input.next(); 
		int count = -1;
		
		if ( start == start ) {
			while ( count < 100) {
				count = count + 1;
				System.out.println( count );
			
		}
		}	
	}


