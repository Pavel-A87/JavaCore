Task 4 p17
Scanner input = new Scanner(System.in);
		
System.out.println("Vvvedite razmer posledovatelnosti: ");
int n = input.nextInt();


int  l = 0;
int m = 0;
int [] chetArray = new int [n];			// massiv chetnih chisel
int [] nechetArray = new int[n]; 	// massiv nechetnih chisel
int index = 0;


while ( index < n ) { 
	System.out.println("Vvedite chislo: ");
	int x = input.nextInt();
	if (x % 2 == 0) {
		chetArray[l] = x;
		l ++;
		} else {
			nechetArray [m] = x;
			m ++;
		}
	index ++;
}
 index = 0;
 System.out.println("Massiv chetnih chesil");
while (index < n) {
	System.out.println(chetArray[index]);
	index ++;
}
index = 0;
System.out.println("Massiv nechetnih chesil:"); 
while (index < n) {
	System.out.println(nechetArray[index]);
	index ++;
}
					
//---------------------
	
	}
}


