// ne rabotait progeria na dvuznachnost!!!!
Scanner input = new Scanner(System.in);
		
		System.out.println("Zadanie: vivesti na ekran summu vseh 4isel kotorie yavlyayutsya ");
		System.out.println("dvuzna4nimi i natselo delyatsya na 11 v diapazone ot A do B(vvoditsya s ");
		System.out.println("klaviatury)");
		System.out.println("Vvedite 4islo A: ");
		int a = input.nextInt();
		System.out.println("Vvedite 4islo B: ");
		int b = input.nextInt();
		int sum = 0;
		int n = a;
	
		//if (a >= 10 && 100 > a && b >= 10 && 100 > b && a > - 100 && b > - 100 && - 10 >= a && - 10 >= b) {
		while ( n >= a && n <= b ) {
					if ( n % 11 == 0 ) {
						sum = sum + n;
						n = n + 1;
						} 
					else {
						n = n + 1;
						}
		}
		System.out.println( "Summa 4isel = " + sum );
		
		//} else {
	//		System.out.println( "4isla dolzhni bit dvuznachnimi!!!!" );
	//}
		
						
		
		
		
							}
}
