Task2 p3
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite peremennuyu a: ");
		double first = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu b: ");
		double second = input.nextDouble();
		
		System.out.println("Vvedite edinitsu izmereniya: ");
		String word = input.next();
		
		double summ = first + second;

		System.out.println( first + " " + word + " + " + second + " " + word +  " = " + summ + " " + word );
		
	}
}

