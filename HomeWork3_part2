Task3 p15
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Vvedite koeffitsienti urovneniya: ");
		System.out.print("Vvedite koeffitsient a:");
		double a = input.nextDouble(); 
		System.out.print("Vvedite koeffitsient b:");
		double b = input.nextDouble(); 
		System.out.print("Vvedite koeffitsient c:");
		double c = input.nextDouble(); 

		double discrim = b * b - 4 * a * c;
				
		boolean koef = a != 0;
		boolean korni = koef && discrim < 0;

		System.out.println("Vvedennie koeffitsienti podhodyat? - " + koef); 
		System.out.println("U urovneniya est korni? - " + korni);
		
	}
}


