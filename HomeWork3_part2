Task3 p11
package letion3day1;
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite  tri storing treugolnika ");
		double  a = input.nextDouble();
		double far = a * 9 / 5 + 32;
		double kel =a + 273.15;

		System.out.println( "Temperatura v Faringeitah: " + far );
		System.out.println( "Temperatura v Kelvinah: " + kel );

	}
}

