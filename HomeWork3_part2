Task2 p1
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite pervoe slovo:");
		String wordone = input.next();
		
		System.out.println("Vvedite vtoroe slovo:");
		String wordtwo = input.next();
		
		System.out.println("Vvedite tretie slovo:");
		String wordthree = input.next();
		
		System.out.println("Vvedite 4etrvertoe slovo:");
		String wordfore = input.next();
		
		System.out.println("Vvedite piatoe slovo:");
		String wordfive = input.next();
		
		
		
		System.out.println( "Itogovaia fraza: " + wordone + " " + wordtwo + " " + wordthree + " " + wordfore + " " + wordfive );
		
	}
}
