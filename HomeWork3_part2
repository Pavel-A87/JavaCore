Task2 p9
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite peremennuyu a: ");
		double  a = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu b: ");
		double b = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu c: ");
		double c = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu d: ");
		double  d = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu e: ");
		double  e = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu f: ");
		double  f = input.nextDouble();
		
		double x = a;
		a = f;
		System.out.println( a );
		
		double y = b;
		b = e;
		System.out.println( b );
		
		double z = c;
		c = d;
		System.out.println( c );
		System.out.println( z );
		System.out.println( y );
		System.out.println( x );
	}
}

