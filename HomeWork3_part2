Task2 p6
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite peremennuyu q: ");
		double  q = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu w: ");
		double w = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu e: ");
		double e = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu r: ");
		double  r = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu t: ");
		double  t = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu y: ");
		double  y = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu u: ");
		double  u = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu i: ");
		double  i = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu o: ");
		double  o = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu p: ");
		double  p = input.nextDouble();
		
		boolean ans = ( q + w + e + r + t ) <= Math.pow(p, 2) - Math.pow(o, 2) || q + w + e + r + t + y + u + i + o <= 100;
		

		System.out.println( "Rezultat  " + ans );
		
	}
}

