Task3  p13
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite  ugli treugolnika: ");
		System.out.println("Vvedite  ugol a: ");
		double  a = input.nextDouble();
		System.out.println("Vvedite  ugol b: ");
		double  b = input.nextDouble();
		System.out.println("Vvedite  ugol c: ");
		double  c = input.nextDouble();
		
		if ( a + b + c == 180 ) {
			System.out.println( "Treugolnik suschestvuet" );
		} else {
			System.out.println( "Treugolnik  ne suschestvuet" );
		}
	}
}

