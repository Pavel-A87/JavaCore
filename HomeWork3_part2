Task3 p14
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite  dva znacheniya dlya sravneniya. ");
		System.out.println("Vvedite  znachenie A v duimah: ");
		double  a = input.nextDouble();
		System.out.println("Vvedite  znachenie B v kilometrah: ");
		double  b = input.nextDouble();
		
		boolean r = a * 2.54 >= b * 100000;
			System.out.println( r );
		
	}
}

