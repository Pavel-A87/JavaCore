Task3 p12
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite  tri storing treugolnika: ");
		System.out.println("Vvedite  storonu a: ");
		double  a = input.nextDouble();
		System.out.println("Vvedite  storonu b: ");
		double  b = input.nextDouble();
		System.out.println("Vvedite  storonu c: ");
		double  c = input.nextDouble();
		
		if (a > 0 && b > 0 && c >0) {
			System.out.println( "Treugolnik suschestvuet" );
		} else {
			System.out.println( "Treugolnik  ne suschestvuet" );
		}
	}
}

