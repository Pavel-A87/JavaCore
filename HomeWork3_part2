Task2 p5
import java.util.*;
public class Task1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vvedite peremennuyu q: ");
		double  q = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu w: ");
		double w = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu e: ");
		double e = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu r: ");
		double  r = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu t: ");
		double  t = input.nextDouble();
		
		System.out.println("Vvedite peremennuyu y: ");
		double  y = input.nextDouble();
		
		boolean x = ( r + t + y) % w > (q - t) || w * w + r * r > (q + y) * (q + y) || e == 10;
		

		System.out.println( "Rezultat  " + x );
		
	}
}

